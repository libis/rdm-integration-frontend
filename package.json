{
  "name": "datasync",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:ci": "ng test --watch=false --browsers=ChromeHeadless --code-coverage",
    "lint": "eslint \"src/**/*.{js,ts}\" && npm run lint:unused-exports",
    "lint:unused-exports": "node scripts/check-unused-exports.mjs",
    "lint:fix": "eslint \"src/**/*.{js,ts}\" --fix",
    "pretty": "prettier --write \"./**/*.{js,jsx,mjs,cjs,ts,tsx,json,html,css,scss}\"",
    "pretty:check": "prettier --check \"./**/*.{js,jsx,mjs,cjs,ts,tsx,json,html,css,scss}\"",
    "analyze": "ng build --configuration production --source-map && npx webpack-bundle-analyzer dist/datasync/*.js",
    "serve:prod": "ng serve --configuration production"
  },
  "private": true,
  "resolutions": {
    "typescript": "~5.8.3"
  },
  "dependencies": {
    "@angular/animations": "^21.1.2",
    "@angular/cli": "^21.1.2",
    "@angular/common": "^21.1.2",
    "@angular/compiler": "^21.1.2",
    "@angular/core": "^21.1.2",
    "@angular/forms": "^21.1.2",
    "@angular/localize": "^21.1.2",
    "@angular/platform-browser": "^21.1.2",
    "@angular/platform-browser-dynamic": "^21.1.2",
    "@angular/router": "^21.1.2",
    "@ng-bootstrap/ng-bootstrap": "^20.0.0",
    "@popperjs/core": "^2.11.8",
    "@primeuix/themes": "^2.0.3",
    "@ro-kit/ui-widgets": "^1.0.40",
    "bootstrap": "^5.3.8",
    "jsonld": "^9.0.0",
    "lit": "^3.3.2",
    "n3": "^2.0.1",
    "ngx-autosize": "^2.0.4",
    "primeflex": "^4.0.0",
    "primeicons": "^7.0.0",
    "primeng": "^21.1.1",
    "rdfxml-streaming-parser": "^3.2.0",
    "rxjs": "^7.8.2",
    "shacl-engine": "^1.1.0",
    "tslib": "^2.8.1",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^21.1.2",
    "@angular/build": "^21.1.2",
    "@angular/compiler-cli": "^21.1.2",
    "@eslint/js": "^8.57.1",
    "@types/jasmine": "^6.0.0",
    "@types/n3": "^1.26.1",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "eslint": "^8.57.1",
    "eslint-plugin-deprecation": "^3.0.0",
    "eslint-plugin-html": "^8.1.3",
    "globals": "^17.2.0",
    "jasmine-core": "^6.0.1",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.2.0",
    "prettier": "^3.8.1",
    "terser": "^5.46.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.32.1",
    "webpack-bundle-analyzer": "^5.2.0"
  }
}

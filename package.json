{
  "name": "datasync",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:ci": "ng test --watch=false --browsers=ChromeHeadless --code-coverage",
    "lint": "eslint \"src/**/*.{js,ts}\"",
    "lint:fix": "eslint \"src/**/*.{js,ts}\" --fix",
    "pretty": "prettier --write \"./**/*.{js,jsx,mjs,cjs,ts,tsx,json,html,css,scss}\"",
    "pretty:check": "prettier --check \"./**/*.{js,jsx,mjs,cjs,ts,tsx,json,html,css,scss}\"",
    "analyze": "ng build --configuration production --source-map && npx webpack-bundle-analyzer dist/datasync/*.js",
    "serve:prod": "ng serve --configuration production"
  },
  "private": true,
  "resolutions": {
    "typescript": "~5.8.3"
  },
  "dependencies": {
    "@angular/animations": "^20.1.4",

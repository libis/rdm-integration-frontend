    "@angular/platform-browser": "^20.1.4",
    "@angular/platform-browser-dynamic": "^20.1.4",
    "@angular/router": "^20.1.4",
    "@ng-bootstrap/ng-bootstrap": "^19.0.1",
    "@popperjs/core": "^2.11.8",
    "@primeng/themes": "^20.0.1",
    "@ro-kit/ui-widgets": "^1.0.0",
    "@ulb-darmstadt/shacl-form": "^2.0.0-rc14",
    "bootstrap": "^5.3.6",
    "jsonld": "^8.3.2",
    "lit": "^3.3.1",
    "n3": "^1.26.0",
    "ngx-autosize": "^2.0.4",
    "npm": "^11.4.0",
    "primeflex": "^4.0.0",
    "primeicons": "^7.0.0",
    "primeng": "^20.0.1",
    "rdfxml-streaming-parser": "^3.1.0",
    "rxjs": "^7.8.2",
    "shacl-engine": "^1.0.2",
